<Window x:Class="Dianzhu.CSClient.WPFView.FormMain"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dianzhu.CSClient.WPFView"
        mc:Ignorable="d"
         WindowStartupLocation="CenterScreen"
        Title="一点办111" Height="800" Width="674"  >

    <Grid >
        <WrapPanel Height="100" Margin="0,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Background="#FFEEEEEE">
            <WrapPanel.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform Y="-3"/>
                </TransformGroup>
            </WrapPanel.RenderTransform>
        </WrapPanel>
        <Grid Margin="0,110,0,0">
            <GridSplitter x:Name="gridSplitter" HorizontalAlignment="Left" Height="660" Margin="180,10,0,-10" VerticalAlignment="Top" Width="5"/>
            <GridSplitter x:Name="gridSplitter1" HorizontalAlignment="Left" Height="640" Margin="400,10,0,0" VerticalAlignment="Top" Width="5"/>
        </Grid>
    </Grid>
</Window>
