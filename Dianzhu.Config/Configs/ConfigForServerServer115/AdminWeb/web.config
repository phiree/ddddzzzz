<configuration>
  <connectionStrings>

    <add name="DianzhuConnectionString"
         connectionString="data source=business.ydban.cn;uid=root;pwd=root;database=dianzhu_publish_test"
         providerName="MySql.Data.MySqlClient" />

  </connectionStrings>

  <appSettings>
    <!--cdn 网址，不末尾不要加斜杠（/）-->
    <add key="cdnroot" value="http://business.ydban.cn:886/"/>



    <add key="SecurityKey" value="1qaz2wsx3edc4rfv"/>


    <add key="all.business_image_root" value="/media/business/"/>
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.0" />
    <authentication mode="Forms" >
      <forms loginUrl="login.aspx"
        name=".ASPXFORMSAUTH" />
    </authentication>
    <membership defaultProvider="DZMembershipProvider" >
      <providers>
        <clear/>
        <add name="DZMembershipProvider" type="Dianzhu.BLL.DZMembershipProvider" connectionStringName="DianzhuConnectionString"
             maxInvalidPasswordAttempts="5" minRequiredPasswordLength="6"
              minRequiredNonalphanumericCharacters="0"
             passwordAttemptWindow="10" applicationName="/"/>
      </providers>
    </membership>
    <pages clientIDMode="Static">
      <controls>
        <add tagPrefix="UC" namespace="Wuqi.Webdiyer" assembly="AspNetPager"  />
      </controls>
    </pages>

  </system.web>

  <log4net>
    <root>
      <level value="Debug"/>

    </root>

    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="logs/%property{log4net:HostName}_log.log" />
      <param name="AppendToFile" value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="10"/>
      <maximumFileSize value="1MB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%date [%thread] %-5level %logger - %message%newline"/>
      </layout>
    </appender>
    <appender name="LogFileAppenderNhibernate" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="logs/%property{log4net:HostName}_nhibernate.log" />
      <param name="AppendToFile" value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="10"/>
      <maximumFileSize value="1MB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%date [%thread] %-5level %logger - %message%newline"/>
      </layout>
    </appender>
    <logger name="Dianzhu">
      <appender-ref ref="LogFileAppender"/>
    </logger>
    <logger name="NHibernate">
      <level value="Info"/>
      <appender-ref ref="LogFileAppenderNhibernate"/>
    </logger>
  </log4net>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-8.0.0.0" newVersion="8.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>