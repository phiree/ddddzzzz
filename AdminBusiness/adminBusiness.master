        $("#me5").attr("src", "image/xiaoxi_1.png");

    }
    $(document).ready(function () {
        $("#me1").click(function () {
            meinit(m1);
            $("#me1").attr("src", "image/dianpuxinxi_2.png");

        });
        $("#me2").click(function () {
