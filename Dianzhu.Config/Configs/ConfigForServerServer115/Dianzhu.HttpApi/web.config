<configuration>
 
  <connectionStrings>

    <add name="DianzhuConnectionString"
         connectionString="data source=business.ydban.cn;uid=root;pwd=root;database=dianzhu_publish_test"
         providerName="MySql.Data.MySqlClient" />

  </connectionStrings>
  <appSettings>
    <!--cdn 网址，不末尾不要加斜杠（/）-->
    <add key="cdnroot" value="http://business.ydban.cn:886/"/>

    <add key="PayServer" value="http://business.ydban.cn:8168/"/>
    <add key="MediaUploadUrl" value="http://business.ydban.cn:8038/uploadfile.ashx"/>
    <add key="MediaGetUrl" value="http://business.ydban.cn:8038/getfile.ashx?filename="/>
    <add key="SecurityKey" value="1qaz2wsx3edc4rfv"/>

    <!--openfire restapi  获取在线会话 url地址-->
    <add key="OpenfireRestApiSessionListUrl" value="http://business.ydban.cn:9090/plugins/restapi/v1/sessions/"/>
    <!--openfire restapi  用户验证key-->
    <add key="OpenfireRestApiAuthKey" value="an4P0ja6v3rykV4H"/>


    <add key="NotifyServer" value="http://business.ydban.cn:8039/"/>



    <!-- 百度地图Geocoding API服务地址 -->
    <add key="BaiduGeocodingAPI" value ="http://api.map.baidu.com/geocoder/v2/?ak="/>
    <add key="BaiduTranAPI" value ="http://api.map.baidu.com/geoconv/v1/?ak="/>
    <add key ="BaiduGeocodingAK" value ="SDHO8UtRNvOl4Cc29KA74UxF"/>
    <add key ="BaiduTranAK" value ="McW4pZayH2PZyWqczoqj2xaV"/>

    <add key ="LocalMediaSaveDir" value ="/localmedia/"/>
    <!--订单过期时间 分钟-->
    <add key="OrderTimeout" value="120"/>

    <!--微信api相关的url-->
    <add key ="WeChatAccessTokenUrl" value="https://api.weixin.qq.com/sns/oauth2/access_token?"/>
    <add key ="WeChatRefreshTokenUrl" value="https://api.weixin.qq.com/sns/oauth2/refresh_token?"/>
    <add key ="WeChatUserInfoUrl" value ="https://api.weixin.qq.com/sns/userinfo?"/>

    <!--新浪微博api相关的url-->
    <add key ="WeiboAccessTokenUrl" value="https://api.weibo.com/oauth2/access_token"/>
    <add key ="WeiboTokenUrl" value ="https://api.weibo.com/oauth2/get_token_info"/>
    <add key ="WeiboBackUrl" value ="https://api.weibo.com/oauth2/default.html"/>
    <add key ="WeiboUserUrl" value ="https://api.weibo.com/2/users/show.json?"/>

    <!--QQ api相关的url-->
    <add key ="QQOpenidUrl" value ="https://graph.qq.com/oauth2.0/me?"/>
    <add key ="QQUserInfoUrl" value ="https://graph.qq.com/user/get_user_info?"/>

  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.0" />
    <authentication mode="Forms">
      <forms name=".ASPXFORMSAUTH" />
    </authentication>
    <membership defaultProvider="DZMembershipProvider">
      <providers>
        <clear />
        <add name="DZMembershipProvider" type="Dianzhu.BLL.DZMembershipProvider" connectionStringName="DianzhuConnectionString" maxInvalidPasswordAttempts="5" minRequiredPasswordLength="6" minRequiredNonalphanumericCharacters="0" passwordAttemptWindow="10" applicationName="/" />
      </providers>
    </membership>
  </system.web>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,Log4net" />
  </configSections>
  <log4net>
    <root>
      <level value="Debug"/>

    </root>

    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="logs/%property{log4net:HostName}_log.log" />
      <param name="AppendToFile" value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="10"/>
      <maximumFileSize value="1MB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%date [%thread] %-5level %logger - %message%newline"/>
      </layout>
    </appender>
    <appender name="LogFileAppenderNhibernate" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="logs/%property{log4net:HostName}_nhibernate.log" />
      <param name="AppendToFile" value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="10"/>
      <maximumFileSize value="1MB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%date [%thread] %-5level %logger - %message%newline"/>
      </layout>
    </appender>
    <logger name="Dianzhu">
      <appender-ref ref="LogFileAppender"/>
    </logger>
    <logger name="NHibernate">
      <level value="Info"/>
      <appender-ref ref="LogFileAppenderNhibernate"/>
    </logger>
  </log4net>
 
  <runtime>
 
 
        <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
 
 
              <dependentAssembly>
 
 
                    <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
 
 
                    <bindingRedirect oldVersion="0.0.0.0-8.0.0.0" newVersion="8.0.0.0" />
 
 
              </dependentAssembly>
 
 
        </assemblyBinding>
 
 
  </runtime>
  <system.webServer>
    <httpProtocol>
      <customHeaders>
        <add name="Access-Control-Allow-Origin" value="*" />
        <add name="Access-Control-Allow-Headers" value="Content-Type" />
        <add name="Access-Control-Allow-Methods" value="GET, POST, PUT, DELETE, OPTIONS" />
      </customHeaders>
    </httpProtocol>
  </system.webServer>

</configuration>
