<script type="text/javascript">
    var m1 = true;
    var m2 = false;
    var m3 = false;
    var m4 = false;
    var m5 = false;
    function meinit(str) {
        m1 = false;
        m2 = false;
        m3 = false;
        m4 = false;
        m5 = false;
        str = true;
        $("#me1").attr("src", "image/dianpuxinxi_1.png");
